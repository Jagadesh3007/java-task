import java.util.*;
class m{
    public static void main(String args[]){
        String str="rgbbgr";
        String str2="";
        for(int i=0;i<str.length();i++){
            if(i<str.length()-1 && str.charAt(i)==str.charAt(i+1)){
                i+=1;
        }else{
            str2+=str.charAt(i);
            }   
        } 
        System.out.println(str2);
       
    }
}
